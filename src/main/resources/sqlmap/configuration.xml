<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>

    <settings>
        <setting name="cacheEnabled"              value="true"/>     <!-- mybatis cache 사용여부 -->
        <setting name="lazyLoadingEnabled"        value="true"/>     <!-- 지연로딩 사용여부 -->
        <setting name="multipleResultSetsEnabled" value="true"/>     <!-- 한 개의 구문에서 여러 개의 ResultSet을 허용할지 여부 -->
        <setting name="defaultExecutorType"       value="REUSE"/>    <!-- 디폴트 Executor 설정(SIMPLE은 특별히 동작하는 것은 업음) -->
        <setting name="defaultStatementTimeout"   value="25000"/>     <!-- DB 응답 타임아웃 설정 -->
        <setting name="callSettersOnNulls" 		  value="false"/>     <!-- 검색 결과 NULL 입력 허용 -->
        <setting name="returnInstanceForEmptyRow" value="false"/>     <!-- ROW가 NULL일 경우 입력 허용 -->
        <setting name="jdbcTypeForNull"           value="NULL" />    <!-- mybatis로 넘어오는 parameter가 null인 경우, jdbcType을 Setting -->

        <!--		<setting name="useColumnLabel"            value="true"/>      &lt;!&ndash; 컬럼명 대신 컬럼 라벨을 사용 &ndash;&gt;-->
        <!--		<setting name="useGeneratedKeys"          value="false"/>     &lt;!&ndash; 생성키에 대한 JDBC 지원 허용 여부 &ndash;&gt;-->
        <!--		<setting name="safeRowBoundsEnabled"      value="false" />    &lt;!&ndash; 중첩구문내 RowBound 사용 허용여부 &ndash;&gt;-->
        <!--		<setting name="mapUnderscoreToCamelCase"  value="true" />    &lt;!&ndash; 전통적 DB 컴럼명을 JAVA의 Camel표기법으로 자동 매핑 설정 &ndash;&gt;-->
        <!--		<setting name="localCacheScope"           value="SESSION" />&lt;!&ndash; 로컬캐시 사용여부(SESSION: 세션을 사용해서 모든쿼리를 캐시) &ndash;&gt;-->
        <!--		<setting name="lazyLoadTriggerMethods" value="equals,clone,hashCode,toString" />&lt;!&ndash; 지연로딩을 야기하는 객체의 메소드를 명시 &ndash;&gt;-->
    </settings>

    <typeAliases>
<!--        <typeAlias type="kr.go.culture.batch.model.member.Member" alias="Member"/>-->
    </typeAliases>
</configuration>